{
  "name": "reportportal.github.io",
  "version": "2.0.0",
  "description": "Landing for ReportPortal",
  "scripts": {
    "predev": "node ./bin/gen-antd-css.js && gatsby develop -p 8080",
    "dev": "gatsby develop -p 8080",
    "graphql:win": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "prebuild": "cross-env NODE_ENV=production node ./bin/gen-antd-css.js",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "setup": "node ./bin/setup.js",
    "gatsby-provision": "gatsby-provision-contentful --contentful-data-path='./contentful/export.json' --space-id=$CONTENTFUL_SPACE_ID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN",
    "lint": "eslint . --ext .js,.jsx --cache",
    "format:eslint": "npm run lint -- --fix",
    "format:prettier": "prettier --write \"**/*.scss\"",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/cssinjs": "^1.16.1",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/static-style-extract": "^1.0.1",
    "@contentful/rich-text-plain-text-renderer": "*",
    "@svgr/webpack": "^7.0.0",
    "ahooks": "^3.7.4",
    "antd": "^5.1.7",
    "classnames": "^2.3.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "formik": "^2.4.3",
    "framer-motion": "^10.0.1",
    "gatsby": "^5.5.0",
    "gatsby-plugin-google-tagmanager": "^5.7.0",
    "gatsby-plugin-image": "^3.5.0",
    "gatsby-plugin-react-helmet": "^6.5.0",
    "gatsby-plugin-sass": "^6.5.0",
    "gatsby-plugin-sharp": "^5.8.1",
    "gatsby-plugin-sitemap": "^6.12.0",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-contentful": "^8.5.0",
    "gatsby-source-graphql": "^5.5.0",
    "gatsby-transformer-sharp": "^5.5.0",
    "gh-pages": "^3.1.0",
    "jotai": "^2.0.2",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-fast-marquee": "^1.3.5",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.5.2",
    "react-responsive": "^9.0.2",
    "react-scroll": "^1.8.9",
    "reading-time": "^2.0.0-1"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.10",
    "axios": "^1.4.0",
    "chalk": "^5.3.0",
    "contentful-import": "^8.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-provision-contentful": "0.0.3",
    "husky": "^8.0.3",
    "inquirer": "^9.2.7",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.3",
    "sass": "^1.59.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2",
    "yargs-parser": "^21.1.1"
  },
  "peerDependencies": {
    "@contentful/rich-text-plain-text-renderer": "*",
    "@gatsbyjs/reach-router": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
