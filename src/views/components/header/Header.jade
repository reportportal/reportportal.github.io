.header-content-wrapper
  .header-content
    .button-sideblock-icon-open(data-js-toggle-sideblock)
      .sideblock-icon-open-part
      .sideblock-icon-open-part
      .sideblock-icon-open-part
    .logo
    .right-block
      .social-block(data-js-social-container)
      .git.social(data-js-top-github-stars-container, data-js-href="https://github.com/reportportal/reportportal")
      span.menu-item.menu-item__documentation(data-js-link="documentation") Documentation
      span.menu-item.menu-item__index(data-js-link="") Overview
.side-content
  .top-block
    .title MENU
    .menu-item.menu-item__index(data-js-link="") Overview
    .menu-item.menu-item__documentation(data-js-link="documentation") Documentation
  .bottom-block
    .title SHARE
    .social-block
      .row
        .git.social(data-js-github-stars-container, data-js-href="https://github.com/reportportal/reportportal")
        .slack.social(data-js-href="https://reportportal.slack.com/")
        .facebook.social(data-js-href="https://www.facebook.com/ReportPortal.io")
      .row
        .twitter.social(data-js-href="http://twitter.com/ReportPortal_io")
        .youtube.social(data-js-href="https://www.youtube.com/channel/UCsZxrHqLHPJcrkcgIGRG-cQ")
        .vk.social(data-js-href="https://vk.com/reportportal_io")
.side-content-close(data-js-side-content-close)
