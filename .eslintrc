{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true
    }
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.babel.js"
      }
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-unused-expressions": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "import/extensions": 0,
    "no-use-before-define": 0,
    "import/first": 0
  }
}
