{
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true
    }
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.babel.js"
      }
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-unused-expressions": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-use-before-define": 0,
    "import/first": 0,
    "linebreak-style": 0,
    "import/no-cycle": 0,
    "prefer-destructuring": 0,
    "import/order": 0,
    "prefer-object-spread": 0,
    "arrow-parens": 0

  }
}
